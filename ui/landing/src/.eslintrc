{
  "rules": {
    "max-classes-per-file": 0,
    "no-undef": 0,
    "no-multi-assign": 0,
    "consistent-return": 0,
    "no-empty": 0,
    "no-plusplus": 0,
    "no-mixed-operators": 0,
    "class-methods-use-this": 0,
    "prefer-const": 0,
    "no-bitwise": 0,
    "max-len": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0
  }
}
